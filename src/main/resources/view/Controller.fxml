<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPaneParent" prefHeight="600.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Controller">
    <children>
      <ImageView fitHeight="603.0" fitWidth="968.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/bg2.png" />
         </image>
      </ImageView>
        <Button fx:id="SkillButton" layoutX="280.0" layoutY="300.0" mnemonicParsing="false" onAction="#SkillTestController" prefHeight="90.0" prefWidth="450.0" styleClass="button2" stylesheets="@graphic.css" text="Contest" />
      <ImageView fitHeight="65.0" fitWidth="68.0" layoutX="900.0" onMouseClicked="#setting" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/setting%20(1).png" />
         </image>
      </ImageView>
        <Button fx:id="LearnButton" layoutX="280.0" layoutY="169.0" mnemonicParsing="false" onAction="#LearnEngController" prefHeight="90.0" prefWidth="450.0" styleClass="button2" stylesheets="@graphic.css" text="Learn English" />
      <Button layoutX="280.0" layoutY="430.0" mnemonicParsing="false" onAction="#Exercise" prefHeight="90.0" prefWidth="450.0" styleClass="button2" stylesheets="@graphic.css" text="Exercise" />
    </children>
</AnchorPane>
